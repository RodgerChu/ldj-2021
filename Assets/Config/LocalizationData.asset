    - "\u0418\u0421\u041F\u042B\u0422\u0410\u041D\u0418\u0415 \u21164: \u0423\u0411\u0418\u0422\u042C
      \u041A\u0420\u041E\u041B\u0418\u041A\u0410"
    - "\u0418\u0421\u041F\u042B\u0422\u0410\u041D\u0418\u0415 \u21165: \u0412\u0415\u0420\u041D\u0423\u0422\u042C\u0421\u042F
      \u0414\u041E\u041C\u041E\u0419"
    - "\u041E\u0439, \u043F\u0440\u0438\u0432\u0435\u0442"
    - "\u041F\u0440\u0438\u0432\u0435\u0442! \u041A\u0440\u0430\u0441\u0438\u0432\u044B\u0439
      \u0441\u0432\u0435\u0442\u043B\u044F\u0447\u043E\u043A. \u0417\u043D\u0430\u0435\u0448\u044C,
      \u0432 \u043B\u0435\u0441\u0443 \u0442\u0430\u043A\u0438\u0445 \u0435\u0449\u0451
      \u043C\u043D\u043E\u0433\u043E. \u041F\u043E\u0439\u0434\u0451\u043C \u043D\u0430\u043B\u043E\u0432\u0438\u043C
      \u0446\u0435\u043B\u0443\u044E \u0431\u0430\u043D\u043A\u0443!"
    - "\u041D\u0435\u0442, \u043C\u043D\u0435 \u043D\u0443\u0436\u043D\u043E \u0432\u0435\u0440\u043D\u0443\u0442\u044C\u0441\u044F
      \u043A \u043C\u043E\u0438\u043C \u0440\u043E\u0434\u0438\u0442\u0435\u043B\u044F\u043C,
      \u043E\u043D\u0438 \u043C\u0435\u043D\u044F \u043F\u043E\u0442\u0435\u0440\u044F\u044E\u0442"
    - "\u0414\u0430 \u043B\u0430\u0434\u043D\u043E \u0442\u0435\u0431\u0435! \u042D\u0442\u043E
      \u0431\u0443\u0434\u0435\u0442 \u0432\u0435\u0441\u0435\u043B\u043E, \u043E\u0431\u0435\u0449\u0430\u044E!
      \u0410 \u043A\u0430\u043A\u043E\u0439 \u043A\u0440\u0430\u0441\u0438\u0432\u044B\u0439
      \u043F\u043E\u0434\u0430\u0440\u043E\u043A \u0442\u044B \u0441\u043C\u043E\u0436\u0435\u0448\u044C
      \u043F\u043E\u0434\u0430\u0440\u0438\u0442\u044C \u0441\u0432\u043E\u0435\u0439
      \u043C\u0430\u043C\u0435!"
    - "\u042F \u043D\u0435 \u0437\u043D\u0430\u044E, \u043C\u043E\u044F \u043C\u0430\u043C\u0430...\u043E\u043D\u0430
      \u0432\u0435\u0434\u044C \u0438 \u043F\u0440\u0430\u0432\u0434\u0430 \u043E\u0431\u0440\u0430\u0434\u0443\u0435\u0442\u0441\u044F,
      \u044F \u043B\u044E\u0431\u043B\u044E \u0435\u0439 \u0434\u0435\u043B\u0430\u0442\u044C
      \u043F\u043E\u0434\u0430\u0440\u043A\u0438\r"
    - "\u0412\u043E\u0442 \u0432\u0438\u0434\u0438\u0448\u044C! \u0411\u0435\u0440\u0438
      \u0431\u0430\u043D\u043A\u0443 \u0438 \u0438\u0434\u0438 \u043F\u043E \u0442\u0440\u043E\u043F\u0438\u043D\u043A\u0435,
      \u0442\u0430\u043C \u0442\u044B \u0441\u0440\u0430\u0437\u0443 \u043D\u0430\u0439\u0434\u0451\u0448\u044C
      \u043C\u043D\u043E\u0433\u043E-\u043C\u043D\u043E\u0433\u043E \u0441\u0432\u0435\u0442\u043B\u044F\u0447\u043A\u043E\u0432!"
    - "\u0412\u0435\u0441\u0435\u043B\u043E, \u043F\u0440\u0430\u0432\u0434\u0430?"
    - "\u0414\u0430! \u041C\u0430\u043C\u0435 \u043E\u0447\u0435\u043D\u044C \u043F\u043E\u043D\u0440\u0430\u0432\u0438\u0442\u0441\u044F.
      \u0422\u0435\u043F\u0435\u0440\u044C \u043D\u0443\u0436\u043D\u043E \u0438\u0434\u0442\u0438
      \u043A \u043D\u0435\u0439"
    - "\u0410\u0445, \u043E\u0431 \u044D\u0442\u043E\u043C. \u042F \u043D\u0435 \u043C\u043E\u0433\u0443
      \u0442\u0435\u0431\u044F \u043E\u0442\u043F\u0443\u0441\u0442\u0438\u0442\u044C,
      \u0442\u0435\u043F\u0435\u0440\u044C \u0442\u044B \u043C\u043E\u0439 \u0433\u043E\u0441\u0442\u044C
      \u0438 \u0434\u043E\u043B\u0436\u0435\u043D \u0432\u044B\u043F\u043E\u043B\u043D\u0438\u0442\u044C
      \u0435\u0449\u0451 \u0447\u0435\u0442\u044B\u0440\u0435 \u0438\u0441\u043F\u044B\u0442\u0430\u043D\u0438\u044F
      \u041B\u0435\u0441\u0430"
    - "\u0413\u043E\u0441\u0442\u044C? \u0422\u044B \u0437\u0434\u0435\u0441\u044C
      \u0436\u0438\u0432\u0451\u0448\u044C?"
    - "\u0414\u0430, \u044D\u0442\u043E \u043C\u043E\u0439 \u043F\u0440\u0435\u043A\u0440\u0430\u0441\u043D\u044B\u0439
      \u0434\u043E\u043C. \u041D\u043E \u0437\u0434\u0435\u0441\u044C \u043C\u043D\u043E\u0433\u043E
      \u0441\u0442\u0440\u0430\u0448\u043D\u044B\u0445 \u0438 \u043E\u043F\u0430\u0441\u043D\u044B\u0445
      \u0441\u0443\u0449\u0435\u0441\u0442\u0432"
    - "\u041E\u043D\u0438 \u043C\u043D\u0435 \u043D\u0430\u0432\u0440\u0435\u0434\u044F\u0442?"
    - "\u041D\u0435\u0442, \u0432\u0435\u0434\u044C \u0442\u044B \u0441\u0438\u043B\u044C\u043D\u0435\u0435
      \u0438\u0445! \u041F\u043E\u0440\u0430 \u0438\u0434\u0442\u0438, \u043D\u0435
      \u043F\u043E\u0442\u0435\u0440\u044F\u0439 \u0431\u0430\u043D\u043A\u0443,
      \u043E\u043D\u0430 \u0442\u0435\u0431\u0435 \u0435\u0449\u0451 \u043F\u0440\u0438\u0433\u043E\u0434\u0438\u0442\u0441\u044F.
      \u041F\u043E\u043C\u043D\u0438: \u0447\u0435\u043C \u0433\u043B\u0443\u0431\u0436\u0435
      \u0442\u044B \u0443\u0445\u043E\u0434\u0438\u0448\u044C \u0432 \u043B\u0435\u0441,
      \u0442\u0435\u043C \u043E\u043F\u0430\u0441\u043D\u0435\u0435 \u0434\u043E\u0440\u043E\u0433\u0430"
    - "\u042F \u0422\u0415\u0411\u042F \u0421\u042A\u0415\u041C!"
    - "\u041E\u043D \u043D\u0435 \u043F\u0440\u0435\u0432\u0440\u0430\u0449\u0430\u0435\u0442\u0441\u044F
      \u0432 \u043C\u043E\u043D\u0441\u0442\u0440\u0430!"
    - "\u0410 \u0434\u043E\u043B\u0436\u0435\u043D?"
    - "\u0414\u0430! \u0412 \u043F\u0440\u043E\u0448\u043B\u044B\u0439 \u0440\u0430\u0437
      \u044F \u0435\u0433\u043E \u043E\u0441\u0432\u043E\u0431\u043E\u0434\u0438\u043B,
      \u043E\u043D \u043F\u0440\u0435\u0432\u0440\u0430\u0442\u0438\u043B\u0441\u044F
      \u0432 \u0441\u0442\u0440\u0430\u0448\u043D\u043E\u0435 \u0447\u0443\u0434\u0438\u0449\u0435
      \u0438 \u0445\u043E\u0442\u0435\u043B \u043C\u0435\u043D\u044F \u0441\u044A\u0435\u0441\u0442\u044C!"
    - "\u041E\u0445, \u0442\u044B \u0441\u043E\u0432\u0441\u0435\u043C \u0437\u0430\u043F\u0443\u0442\u0430\u043B\u0441\u044F.
      \u041F\u043E\u0439\u0434\u0451\u043C"
    - "\u041E\u0441\u0442\u0430\u043B\u043E\u0441\u044C \u043F\u043E\u0441\u043B\u0435\u0434\u043D\u0435\u0435
      \u0438\u0441\u043F\u044B\u0442\u0430\u043D\u0438\u0435, \u0441\u0430\u043C\u043E\u0435
      \u0441\u043B\u043E\u0436\u043D\u043E\u0435"
    - "\u0418 \u044F \u0432\u0435\u0440\u043D\u0443\u0441\u044C \u043A \u0440\u043E\u0434\u0438\u0442\u0435\u043B\u044F\u043C?"
    - "\u041F\u043E\u0441\u043B\u0435 \u044D\u0442\u043E\u0433\u043E \u0442\u044B
      \u0431\u0443\u0434\u0435\u0448\u044C \u0434\u043E\u043C\u0430, \u043E\u0431\u0435\u0449\u0430\u044E"
    - "\u0418 \u0432\u0441\u0451?"
    - "\u0418 \u0432\u0441\u0451, \u044D\u0442\u043E \u0431\u0443\u0434\u0435\u0442
      \u043A\u043E\u043D\u0435\u0446 \u0442\u0432\u043E\u0435\u0433\u043E \u043F\u0443\u0442\u0435\u0448\u0435\u0441\u0442\u0432\u0438\u044F"
    - "\u042F \u0441\u0434\u0435\u043B\u0430\u043B, \u044F \u0432\u0441\u0451 \u0441\u0434\u0435\u043B\u0430\u043B,
      \u044F \u043F\u0440\u043E\u0448\u0451\u043B \u0432\u0441\u0435 \u0438\u0441\u043F\u044B\u0442\u0430\u043D\u0438\u044F,
      \u043C\u043E\u0436\u043D\u043E \u043C\u043D\u0435 \u043A \u0440\u043E\u0434\u0438\u0442\u0435\u043B\u044F\u043C,
      \u043F\u043E\u0436\u0430\u043B\u0443\u0439\u0441\u0442\u0430?"
    - "\u041A \u0440\u043E\u0434\u0438\u0442\u0435\u043B\u044F\u043C? \u041D\u043E
      \u0438\u0445 \u0431\u043E\u043B\u044C\u0448\u0435 \u043D\u0435\u0442"
    - "\u0427\u0442\u043E? \u0427\u0442\u043E \u0437\u043D\u0430\u0447\u0438\u0442
      \u201C\u043D\u0435\u0442\u201D?"
    - "\u0412\u0435\u0440\u043D\u0438\u0441\u044C \u0442\u0443\u0434\u0430, \u0433\u0434\u0435
      \u0442\u044B \u0443\u0431\u0438\u043B \u043C\u043E\u043D\u0441\u0442\u0440\u0430"
    - "\u041C\u0430\u043C\u043E\u0447\u043A\u0430! \u041D\u0435\u0442, \u044F \u043D\u0435
      \u0432\u0435\u0440\u044E, \u0442\u044B \u043C\u0435\u043D\u044F \u043E\u043F\u044F\u0442\u044C
      \u043E\u0431\u043C\u0430\u043D\u044B\u0432\u0430\u0435\u0448\u044C!"
    - "\u0422\u0430\u043A \u0431\u044B\u043B\u043E \u043D\u0443\u0436\u043D\u043E"
    - "\u0417\u0430\u0447\u0435\u043C?!"
    - "\u041F\u043E\u0442\u043E\u043C\u0443 \u0447\u0442\u043E \u0442\u044B \u0442\u0430\u043A\u043E\u0439
      \u0436\u0435 \u043A\u0430\u043A \u043C\u044B, \u0442\u044B \u043D\u0438\u043A\u043E\u0433\u0434\u0430
      \u043D\u0435 \u0431\u044B\u043B \u201C\u0438\u0445\u201D \u0440\u0435\u0431\u0451\u043D\u043A\u043E\u043C.
      \u0422\u044B \u0434\u043E\u043B\u0436\u0435\u043D \u0431\u044B\u043B \u0432\u0435\u0440\u043D\u0443\u0442\u044C\u0441\u044F
      \u0432 \u043B\u0435\u0441. \u0412\u0435\u0440\u043D\u0443\u0442\u044C\u0441\u044F
      \u0434\u043E\u043C\u043E\u0439. \u0422\u0435\u043F\u0435\u0440\u044C \u0442\u044B
      \u0434\u043E\u043C\u0430."
    - "\u041D\u0430\u0436\u043C\u0438\u0442\u0435 \u0415"
    - "\u0413\u0440\u043E\u043C\u043A\u043E\u0441\u0442\u044C \u043C\u0443\u0437\u044B\u043A\u0438"
